# SQLite 配置
DB_SQLITE_PATH=./database.sqlite

# MySQL 配置 (当 DB_TYPE=mysql 时生效)
DB_HOST=127.0.0.1
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_NAME=your_database_name